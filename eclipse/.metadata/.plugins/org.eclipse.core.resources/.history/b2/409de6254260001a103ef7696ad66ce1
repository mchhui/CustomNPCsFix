import org.json.JSONObject;
import org.json.JSONTokener;

import net.minecraft.nbt.NBTTagByte;
import net.minecraft.nbt.NBTTagCompound;
import net.minecraft.nbt.NBTTagDouble;
import net.minecraft.nbt.NBTTagFloat;
import net.minecraft.nbt.NBTTagInt;
import net.minecraft.nbt.NBTTagIntArray;
import net.minecraft.nbt.NBTTagList;
import net.minecraft.nbt.NBTTagString;
import noppes.npcs.util.JsonException;
import noppes.npcs.util.NBTJsonUtil;

public class TestMain {
    public static void main(String[] args) throws FileNotFoundException {
        long time = new Date().getTime();
        File file = new File(
                "E:\\java\\Minecraft-forge\\CustomNPCFix\\eclipse\\saves\\新的世界\\customnpcs\\playerdata\\c1b94a42-98fc-3354-947f-44f786fd56fa.json");
        InputStream inputStream = new FileInputStream(file);
        JSONObject json = new JSONObject(new JSONTokener("{\"test\":[1]}"));
        System.out.println((new Date().getTime() - time));
        NBTTagCompound nbt = new NBTTagCompound();
        nbt.setIntArray("tsetIntArr", new int[] { 1, 2, 3 });
        nbt.setByteArray("testByteArr", new byte[] { 1, 2, 3 });
        NBTTagList list1 = new NBTTagList();
        list1.appendTag(new NBTTagFloat(1));
        nbt.setTag("testIntList", list1);
        list1 = new NBTTagList();
        list1.appendTag(new NBTTagByte((byte) 1));
        nbt.setTag("testByteList", list1);
        list1 = new NBTTagList();
        list1.appendTag(new NBTTagString("hello"));
        nbt.setTag("testStringList", list1);
        list1 = new NBTTagList();
        list1.appendTag(new NBTTagIntArray(new int[] { 1, 2, 3 }));
        nbt.setTag("testIntArrList", list1);
        System.out.println(NBTJsonUtil.Convert(nbt));
        System.out.println(json.get("test"));
    }
    /*public static void main(String[] args) {
